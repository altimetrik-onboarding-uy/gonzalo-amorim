    <validationRules>
        <fullName>Friday_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>Friday__c &gt;= 24</errorConditionFormula>
        <errorDisplayField>Friday__c</errorDisplayField>
        <errorMessage>Daily work input cannot be greater than 24 hours.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Monday_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>Monday__c &gt;= 24</errorConditionFormula>
        <errorDisplayField>Monday__c</errorDisplayField>
        <errorMessage>Daily work input cannot be greater than 24 hours.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Description</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Type__c, &apos;Other&apos;), ISBLANK(Description__c))</errorConditionFormula>
        <errorDisplayField>Description__c</errorDisplayField>
        <errorMessage>Please add a Description.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Thursday_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>Thursday__c &gt;= 24</errorConditionFormula>
        <errorDisplayField>Thursday__c</errorDisplayField>
        <errorMessage>Daily work input cannot be greater than 24 hours.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Tuesday_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>Tuesday__c &gt;= 24</errorConditionFormula>
        <errorDisplayField>Tuesday__c</errorDisplayField>
        <errorMessage>Daily work input cannot be greater than 24 hours.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Date</fullName>
        <active>true</active>
        <description>Validates start date and end date.</description>
        <errorConditionFormula>Start_Date__c &gt;  End_Date__c</errorConditionFormula>
        <errorMessage>Invalid date, Start date must be set prior to End date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Hour_Rate</fullName>
        <active>true</active>
        <errorConditionFormula>Hour_Rate__c &gt;= 100.01</errorConditionFormula>
        <errorMessage>Pay rate invalid, hour rate must be equal or less than $100 per hour.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Wednesday_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>Wednesday__c &gt;= 24</errorConditionFormula>
        <errorDisplayField>Wednesday__c</errorDisplayField>
        <errorMessage>Daily work input cannot be greater than 24 hours.</errorMessage>
    </validationRules>
